{
  "env": {
    "node": true,
    "es2021": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": 0,
    "import/prefer-default-export": "off",
    "consistent-return": "off",
    "global-require": 0,
    "no-console": "off"
  }
}